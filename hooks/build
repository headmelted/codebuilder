#!/bin/sh

docker build --build-arg DOCKER_TAG="$DOCKER_TAG" --build-arg ARCHIE_STRATEGY="$ARCHIE_STRATEGY" --build-arg ARCHIE_ARCH="$ARCHIE_ARCH" --build-arg FROM_IMAGE="${FROM_IMAGE}" --build-arg ARCHIE_HOST_DEPENDENCIES="${ARCHIE_HOST_DEPENDENCIES}" --build-arg ARCHIE_TARGET_DEPENDENCIES="${ARCHIE_TARGET_DEPENDENCIES}" -t "headmelted/archie:$DOCKER_TAG" .
